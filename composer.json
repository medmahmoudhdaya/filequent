{
  "name": "zidbih/filequent",
  "description": "A lightweight file-based ORM for PHP using JSON files, inspired by Eloquent.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Zidbih\\Filequent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zidbih\\Filequent\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Zidbih",
      "email": "medmahmoudhdaya@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.2"
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
